{
  "name": "@juki-team/base-ui",
  "version": "1.2.94",
  "repository": "https://github.com/juki-team/base-ui",
  "author": "juki-team",
  "license": "MIT",
  "type": "module",
  "main": "dist/cjs/main.js",
  "module": "dist/esm/main.js",
  "types": "dist/module-types/index.d.ts",
  "files": [
    "dist",
    "tsconfig.base.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/module-types/src/index.d.ts",
      "import": "./dist/esm/main.js",
      "require": "./dist/cjs/main.js"
    },
    "./server-components": {
      "types": "./dist/module-types/src/components/server/index.d.ts",
      "import": "./dist/esm/server.js",
      "require": "./dist/cjs/server.js"
    },
    "./helpers": {
      "types": "./dist/module-types/src/helpers/index.d.ts",
      "import": "./dist/esm/helpers.js",
      "require": "./dist/cjs/helpers.js"
    },
    "./enums": {
      "types": "./dist/module-types/src/enums/index.d.ts",
      "import": "./dist/esm/enums.js",
      "require": "./dist/cjs/enums.js"
    },
    "./types": {
      "types": "./dist/module-types/src/types/index.d.ts",
      "import": "./dist/esm/types.js",
      "require": "./dist/cjs/types.js"
    },
    "./constants": {
      "types": "./dist/module-types/src/constants/index.d.ts",
      "import": "./dist/esm/constants.js",
      "require": "./dist/cjs/constants.js"
    },
    "./settings": {
      "types": "./dist/module-types/src/settings/index.d.ts",
      "import": "./dist/esm/settings.js",
      "require": "./dist/cjs/settings.js"
    },
    "./styles/mixins": {
      "sass": "./dist/styles/_mixins.scss"
    },
    "./styles.css": {
      "default": "./dist/styles/index.css"
    },
    "./assets/*": "./dist/assets/*"
  },
  "scripts": {
    "storybook": "cross-env NODE_ENV=storybook storybook dev -p 6006",
    "build:storybook": "rm -rf docs && storybook build -o docs",
    "build:rollup-clean": "rm -rf dist && rm -rf node_modules/.cache/rollup-plugin-typescript2 && rollup -c --no-cache --strictDeprecations --environment NODE_ENV:production",
    "build:rollup": "rm -rf dist && rollup -c --strictDeprecations --environment NODE_ENV:production",
    "build:sass": "rm -rf dist/css-styles && sass src/styles:dist/styles --no-source-map --style=compressed",
    "build:publish": "yarn publish --new-version $(node -p \"require('./package.json').version\") --no-git-tag-version",
    "git:commit": "f() { param=${1}; message=${param:-chore}; echo ${message}; git add .; echo $message | git commit -F -; }; f",
    "git:commit-version": "version=\"v$(node -p \"require('./package.json').version\")\" && yarn git:commit $(echo $version) && git tag -a $(echo $version) -m $(echo $version);",
    "git:push": "git push origin main && git push origin --tags",
    "push": "f() { yarn check:circular-deps && yarn git:commit \"${1}\" && yarn version --patch --no-git-tag-version && yarn build:rollup && yarn build:sass && yarn git:commit-version && yarn build:publish && yarn git:push; }; f",
    "push:storybook": "f() { yarn check:circular-deps && yarn git:commit \"${1}\" && yarn version --patch --no-git-tag-version && yarn build:rollup-clean && yarn build:sass && yarn build:storybook && yarn git:commit-version && yarn build:publish && yarn git:push; }; f",
    "pull": "yarn add @juki-team/commons",
    "chromatic": "npx chromatic --project-token=xyz",
    "clean-and-reset": "rm -rf node_modules yarn.lock && yarn cache clean && yarn install",
    "check-style-files": "find src/styles -type f -name \"*.scss\" -exec stat -f \"%z %N\" {} + | awk '{printf \"%.1f KB %s\\n\", $1/1024, $2}' | sort -nr",
    "outdated": "npx npm-check-updates",
    "check:circular-deps": "npx madge --circular ./src/*",
    "check:circular-deps-graph": "npx madge --circular ./dist/* --image deps.png"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.18.7",
    "@codemirror/lang-cpp": "^6.0.3",
    "@codemirror/lang-html": "^6.4.10",
    "@codemirror/lang-java": "^6.0.2",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-markdown": "^6.3.4",
    "@codemirror/lang-python": "^6.2.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@excalidraw/excalidraw": "^0.18.0",
    "@floating-ui/react": "^0.27.16",
    "@handsontable/react-wrapper": "^16.1.1",
    "@hookform/resolvers": "^5.2.2",
    "@juki-team/commons": "^0.0.957",
    "@milkdown/crepe": "^7.16.0",
    "@milkdown/kit": "^7.16.0",
    "@milkdown/react": "^7.16.0",
    "@tanstack/react-virtual": "^3.13.12",
    "@viz-js/lang-dot": "^1.0.5",
    "@viz-js/viz": "^3.18.0",
    "ably": "^2.14.0",
    "codemirror": "^6.0.2",
    "countries-and-timezones": "^3.8.0",
    "diff": "^8.0.2",
    "diff2html": "^3.4.52",
    "dom-to-image-more": "^3.7.1",
    "handsontable": "^16.1.1",
    "highlight.js": "^11.11.1",
    "i18n-iso-countries": "^7.14.0",
    "i18next": "^25.5.2",
    "katex": "^0.16.22",
    "libphonenumber-js": "^1.12.22",
    "microdiff": "^1.5.0",
    "motion": "^12.23.21",
    "react-color": "^2.19.3",
    "react-device-detect": "^2.2.3",
    "react-hook-form": "^7.63.0",
    "react-image-crop": "^11.0.10",
    "react-markdown": "^10.1.0",
    "react-resize-detector": "^12.3.0",
    "react-tooltip": "^5.29.1",
    "recharts": "^3.2.1",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "reveal.js": "^5.2.1",
    "swr": "^2.3.6",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "yup": "^1.7.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/css": "^0.13.0",
    "@eslint/js": "^9.38.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.4.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-docs": "^9.1.8",
    "@storybook/addon-links": "^9.1.8",
    "@storybook/addon-themes": "^9.1.8",
    "@storybook/react-webpack5": "^9.1.8",
    "@swc/core": "^1.13.5",
    "@types/codemirror": "^5.60.16",
    "@types/d3-graphviz": "^2.6.10",
    "@types/graphlib-dot": "^0.6.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^19.1.9",
    "@types/reveal.js": "^5.2.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "rollup": "^4.52.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "storybook": "^9.1.8",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.1",
    "webpack": "^5.101.3"
  },
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*,"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@hpcc-js/wasm": "2.22.4",
    "@types/minimatch": "^5.0.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
