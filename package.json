  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "/dist"
  ],
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "@juki-team/commons": "^0.0.54",
    "@monaco-editor/react": "^4.4.2",
    "codemirror": "^5.65.2",
    "prismjs": "^1.27.0",
    "react-color": "^2.19.3",
    "react-copy-to-clipboard": "^5.1.0",
    "react-google-login": "^5.2.2",
    "react-hook-form": "^7.29.0",
    "react-i18next": "^11.16.6",
    "react-image-crop": "^10.0.0",
    "react-markdown": "^8.0.2",
    "react-modal": "^3.14.4",
    "react-resize-detector": "^7.0.0",
    "react-virtual": "^2.10.4",
    "rehype-katex": "^6.0.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sass": "^1.50.0",
    "socket.io-client": "^4.4.1",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -s public -o docs",
    "rollup:build": "rollup -c",
    "pull": "yarn add @juki-team/commons",
    "build": "rm -rf docs && yarn storybook:build && rm -rf dist && yarn rollup:build",
    "push": "yarn build && yarn publish --patch && git add . && node -p \"require('./package.json').version\" | git commit -F - ; git push origin main && node -p \"require('./package.json').version\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/node-logger": "^6.4.22",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/codemirror": "^5.60.5",
    "@types/jest": "^27.0.1",
    "@types/node": "^17.0.24",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.5",
    "@types/react-color": "^3.0.6",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^18.0.1",
    "@types/react-modal": "^3.13.1",
    "@types/uuid": "^8.3.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "rollup": "^2.70.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "web-vitals": "^2.1.0",
    "webpack": "^5.72.0"
  }
}
